<%- include("partials/header")%>
<div class="example">
    <h1>Home</h1>
    <p><%= homecontent %></p>
    
        <% posts.forEach(function(element){ %>
            <h1><%= element.title %></h1>
            <p><%= element.content.substring(0, 100) + "..." %>
                <a href="/posts/<%=element._id%>">Read More</a>
                <form action="/delete/<%=element._id%>" method="post">
                    <button type='submit' class="btn btn-primary mb-2">Delete</button>
                 </form>
            </p>
             
        <% }) %>
       

    </div>

<%- include("partials/footer")%>

